   <body>
    <%-include('common/header.ejs')-%>
      <div class="container-fluid bg-side-nav">
         <div class="row">
            <div class="col-lg-2 sidebaare">
                <%-include('common/sidebaar.ejs')-%>
               
            </div>
            <div class="col-lg-10">

              <div class="dashboard-content-area">
                <!-- put your contente -->
               <h1>Tambah Pengeluaran</h1>
    
              <form method="post" action="/admin/category/add-category/" enctype="multipart/form-data">             
                  <div class="mb-3 mt-3 row g-9 align-items-center">  
                    <div class="col col-lg-2">
                      <label class="form-label">Judul</label>
                    </div>
                    <div class="col">
                      <input type="text" name ="category" class="form-control" placeholder="Makan keluarga">
                    </div>
                  </div>
                  <div class="mb-3 mt-3 row g-9 align-items-center">  
                    <div class="col col-lg-2">
                      <label class="form-label">Tanggal</label></div>
                    <div class="col"><input type="text" name ="category_url" class="form-control" placeholder="2022-10-24"></div>
                  </div>


                  <div class="mb-3 mt-3 row g-9 align-items-center">  
                    <div class="col col-lg-2">
                      <label class="form-label">Jumlah</label></div>
                    <div class="col"><input type="text" name ="category_title" class="form-control" placeholder="50000"></div>
                  </div>
                  <div class="mb-3 mt-3 row g-9 align-items-center">  
                    <div class="col col-lg-2">
                      <label class="form-label">Status</label></div>
                    <div class="col">
                      <input type="text" name ="category_meta_keyword" class="form-control" placeholder="Hutang">
                    </div>
                  </div>
                  <div class="mb-3 mt-3 row g-9 align-items-center">  
                    <div class="col col-lg-2">
                      <label class="form-label">Deskripsi</label></div>
                    <div class="col"><input type="text" name ="category_meta_desc" class="form-control" placeholder="Tes"></div>
                  </div>
                  <div class="mb-5 mt-3 align-items-center text-center"> 
                       <button type="submit" class="btn btn-success">Submit</button>
                  </div>
              </form>

              <!-- end your content here -->
             </div>

